@model Enrollment

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Create Enrollment</h2>
        </div>
        <div asp-validation-summary="All" />
        <div class="mb-3">
            <label asp-for="StudentId"></label>
            <input asp-for="StudentId" class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="Grade"></label>
            <input asp-for="Grade" class="form-control" />
        </div>
        <button type="submit" class="btn btn-primary">Create </button>
        <a asp-controller="Enrollment" asp-action="Index" class="btn btn-secondary">
            Back to list
        </a>
    </div>
</form>


